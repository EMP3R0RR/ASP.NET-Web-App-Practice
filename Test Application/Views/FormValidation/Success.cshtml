
@{
    ViewBag.Title = "Success";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model Test_Application.Models.FormValidation

<h2>Success</h2>

@if (Model != null)
{
    <h4>@Model.Name</h4>
    <h4>@Model.ID</h4>
    <h4>@Model.Email</h4>
    <h4>@Model.UName</h4>
    <h4>@Model.DOB.ToString("yyyy-MM-dd")</h4>
 //converts the value into that format
    <h4>@Model.Profession</h4>
    <h4>@string.Join(", ", Model.Hobbies)</h4>
    <h4>@Model.Gender</h4>
    
}

