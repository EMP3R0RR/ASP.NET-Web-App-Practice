
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Delete</h2>



<h4> Do you want to delete this item ? @Model.Id</h4>

<form method="post" action="/Product/DeleteConfirmed">
    <input type="hidden" name="id" value="@Model.Id" />
    <button type="submit">Yes</button>
    <a href="/Product/Index">No</a>
</form>






